<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 <configuration>
 	<!-- 자주 사용하는 type에 별칭(alias) 지정하기 -->
 	<typeAliases>
 		<typeAlias type="com.winnie.spring05.member.dto.MemberDto" alias="memberDto"/>
 	</typeAliases>
 	
 	<!-- mapper 설정 -->
 	<mappers>
 		<mapper resource="com/winnie/spring05/mybatis/MemberMapper.xml" />
 	</mappers>
 </configuration>